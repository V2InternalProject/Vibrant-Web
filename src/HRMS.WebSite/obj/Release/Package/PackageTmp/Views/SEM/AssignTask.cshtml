    <div class="tablestyle3">
        <table id="jqDocTable">
        </table>
    </div>
    <div id="jqDocTablePager">
    </div>
</div>
<script type="text/javascript">
     $(document).ready(function () {
        $("#jqDocTable").jqGrid({
            // Ajax related configurations
            url: '@Url.Action("TravelEmergencyContactLoadGrid", "Travel")',
            postData: { travelId: 12 },//hardcoded for testing
            datatype: "json",
            mtype: "POST",
            // Specify the column names
            colNames: ["Document Category", "Document Sub Category", "Document Name (Latest)", "Upload Date", "Size (KB)", "Last Modified", "Review", "History", "Delete"],            
            // Configure the columns
            colModel: [
                { name: "DocCategory", index: "ProjectCode", align: "center", width: 140},
                { name: "DocSubCategory", index: "DocSubCategory", align: "center", width: 140 },
                { name: "DocumentName", index: "DocumentName", align: "center", width: 140 },
                { name: "UploadDate", index: "UploadDate", align: "center", width: 60 },
                { name: "Size", index: "Size", align: "center", width: 30 },
                { name: "LastModified", index: "LastModified", align: "center", width: 60 },
                { name: "Review", index: "Review", align: "center", width: 60 },
                { name: "History", index: "History", align: "center", width: 60 },
                {
                    name: "Delete",
                    index: "Delete",
                    width: 10,
                    align: "center",
                    formatter: function() {
               
                            return '<img src="../../Content/themes/base/images/delete-icon.png" width="15px" height="15px" class="HideinPrint">';
                        
                    }
                    },
               
            ],
            // Grid total width and height
            width: 700,
            // Paging
            toppager: false,
            jsonReader: { repeatitems: false },
            pager: $("#jqDocTablePager"),
            rowNum: 20,
            rowList: [],
            viewrecords: true, // Specify if "total number of records" is displayed
            height: 'auto',
            autowidth: false,
            // Default sorting
            sortname: "",
            sortorder: "asc",
            // Grid caption
            //caption: "Documents",
              gridComplete: function () {
            },
            onCellSelect: function(rowid, iCol) {
			},
        }).navGrid("#jqDocTablePager",
            { search: false, refresh: false, add: false, edit: false, del: false },
            {}, // settings for edit
            {},
            {}
        );
     
     
     });
</script>
