    function AppraisalRatingsReportShow() {
        var id = $('#AppraisalRatingsYearID').val();
        if (id == "") {
            $("#selectAppraisalYear").dialog({
                closeOnEscape: false,
                resizable: false,
                height: 120,
                width: 300,
